dPlaced, setBidPlaced] = useState(null);\n   const [acceptBidOpen, setAcceptBidOpen] = useState({\n      open: false,\n      sellerId: undefined,\n   });\n   const [isBidPopOpen, setIsBidPopOpen] = useState({\n         open: false,\n         edit: false,\n         content: {\n            price: \"\", desc: \"\"\n         }\n      });\n   const [productInfo, setProductInfo] = useState({});\n\n   useEffect(() => {\n      setIsLoading(true);\n      axios.post(\"https://flipin-store.herokuapp.com/getproduct.php\", {pid: id}, authHeader)\n         .then(({data}) => {\n            if(data.responseCode === 200){\n               setProductInfo(data);\n               setBidPlaced(data.bids.findIndex(bid => bid.sellerId === state.id));\n               setIsLoading(false);\n            }\n         })\n         .catch(e => console.log(e));\n   }, [id])\n\n   useEffect(() => {\n      if(isBidPopOpen.open){\n            document.body.style.overflow = 'hidden';\n        } else {\n            document.body.style.overflow = 'unset';\n        }\n   }, [isBidPopOpen]);\n\n   const handlePlaceBid = () => {\n     setIsBidPopOpen(prev => ({\n         ...prev,  \n         open: true,\n         edit: false\n     })); \n   };\n\n   const handleClose = (update = false) => {\n      setIsBidPopOpen(prev => ({\n         ...prev,  \n         open: false,\n     }));\n     if(update){\n      if(bidPlaced !== -1) {\n         let newBids = productInfo.bids;\n         newBids[bidPlaced] = {\n            ...newBids[bidPlaced],\n            amount: update.price,\n            description: update.desc,\n         }\n         setProductInfo(prev => ({\n            ...prev,\n            bids: newBids,\n         }))\n      } else {\n         let updatedBids = productInfo.bids;\n         updatedBids.push({\n            sellerId: state.id,\n            logo: \"\",\n            sellerName: state.name,\n            amount: update.price,\n            description: update.desc\n         });\n         setBidPlaced(updatedBids.length - 1);\n         setProductInfo(prev => ({\n            ...prev,\n            bids: updatedBids,\n         }))\n      }\n     }\n   }\n\n   const handlePublish = (publish) => {\n      axios.post(\"https://flipin-store.herokuapp.com/publish.php\", {pid: id, publish}, authHeader)\n      .then(({data}) => {\n         if(data.responseCode === 204) {\n            setProductInfo(prev => ({\n            ...prev,\n            active: publish\n            }));\n         }\n      })\n      .catch(e => console.error(e));\n   }\n   \n   const handleEditListing = () => {\n      history.push({\n         pathname: '/create',\n         state: {\n         edit: true,\n         pId: id,\n         name: productInfo.name,\n         category: productInfo.category,\n         desc: productInfo.description,\n         src: productInfo.image\n         }\n      });\n   }\n\n   const handleChatWithCustomer = () => {\n      history.push({\n        pathname: \"/inbox\",\n        state: {\n          customer: productInfo.customerId,\n          customerName: productInfo.customerName,\n          seller: state.id,\n          sellerName: state.name,\n          sellerLogo: state.logo ? state.logo : null,\n        },\n      });\n   }\n\n   const handleAcceptBidCancel = () => {\n      setAcceptBidOpen(false);\n   }\n\n   const handleAcceptOrder = () => {\n     axios.post(\"https://flipin-store.herokuapp.com/acceptorder.php\", {\n       sid: acceptBidOpen.sellerId,\n       pid: id,\n     }, authHeader)\n     .then(res => {\n        if(res.data.responseCode === 201){\n          console.log(res.data);\n          setAcceptBidOpen({open: false, sellerId: null})\n          history.push(\"/orders\");\n        } else {\n          console.log(res.data);\n        }\n     })\n     .catch(e => {\n       console.error(e);\n     });\n   }\n\n   return isLoading ? (\n     <div className=\"loader\" style={{ height: \"80vh\" }}>\n       <CircularProgress />\n     </div>\n   ) : (\n     <div className=\"product__page\">\n       {/* Popup for Bid */}\n       <Popup open={isBidPopOpen.open} handleClose={handlePlaceBid}>\n         <BidInput\n           pid={id}\n           edit={isBidPopOpen.edit}\n           content={isBidPopOpen.content}\n           handleClose={handleClose}\n         />\n       </Popup>\n       <Popup open={acceptBidOpen.open} handleClose={handleAcceptBidCancel}>\n         <h1>Accept Bid and Place Order?</h1>\n         <p>Are you sure you want to accept this bid?</p>\n         <div className=\"product-page__info-buttons\">\n           <button onClick={handleAcceptOrder}>\n            <CheckRounded />&nbsp;\n            Yes\n           </button>\n           <button onClick={handleAcceptBidCancel}>\n            <CloseRounded />&nbsp;\n            No\n           </button>\n         </div>\n       </Popup>\n\n       {/* Section 1 - Product Info */}\n       <section className=\"product-page__info\">\n         <img src={productInfo.image} alt={productInfo.name} />\n         <div className=\"product-page__info-text-container\">\n           <h1>{productInfo.name}</h1>\n           <div className=\"product-page__info-loc\">\n             <PinIcon />\n             <h3>{productInfo.location}</h3>\n           </div>\n           {!state.isSeller && (\n             <span\n               className={`p__status ${\n                 productInfo.active ? \"live\" : \"inactive\"\n               }`}\n             >\n               {productInfo.active ? \"LIVE\" : \"INACTIVE\"}\n             </span>\n           )}\n           <div className=\"product-page__info-lb\">\n             <h2>{`₹ ${productInfo.lowestBid}`}</h2>\n             <span>LOWEST BID</span>\n           </div>\n           <div className=\"product-page__info-desc\">\n             <span>Product Description</span>\n             <p>{productInfo.description}</p>\n           </div>\n           <span>{`Posted By: ${productInfo.customerName}`}</span>\n\n           {state.isSeller ? (\n             <div className=\"product-page__info-buttons\">\n               <button disabled={bidPlaced !== -1} onClick={handlePlaceBid}>\n                 <span>+</span>\n                 PLACE BID\n               </button>\n               <button onClick={handleChatWithCustomer}>\n                 <ChatIcon\n                   style={{ fill: \"#fff\", width: \"20px\", marginRight: \"10px\" }}\n                 />\n                 CHAT WITH CUSTOMER\n               </button>\n             </div>\n           ) : (\n             state.id === productInfo.customerId && (\n               <div className=\"product-page__info-buttons\">\n                 <button onClick={handleEditListing}>\n                   <EditRounded\n                     style={{\n                       fill: \"#fff\",\n                       width: \"20px\",\n                       marginRight: \"10px\",\n                     }}\n                   />\n                   EDIT LISTING\n                 </button>\n                 {!productInfo.active ? (\n                   <button onClick={() => handlePublish(true)}>\n                     <VisibilityRounded\n                       style={{\n                         fill: \"#fff\",\n                         width: \"20px\",\n                         marginRight: \"10px\",\n                       }}\n                     />\n                     PUBLISH\n                   </button>\n                 ) : (\n                   <button onClick={() => handlePublish(false)}>\n                     <VisibilityOffRounded\n                       style={{\n                         fill: \"#fff\",\n                         width: \"20px\",\n                         marginRight: \"10px\",\n                       }}\n                     />\n                     UNPUBLISH\n                   </button>\n                 )}\n               </div>\n             )\n           )}\n         </div>\n       </section>\n\n       {/* Section 2 - Other seller bids */}\n\n       {state.isSeller || state.id === productInfo.customerId ? (\n         <>\n           <section className=\"product__sections\">\n             <h2>Other Seller Bids</h2>\n             <div className=\"product__bids\">\n               {productInfo.bids.map((bid) => (\n                 <div\n                   className={`bid__item ${\n                     state.isSeller ? \"per_seller\" : \"per_customer\"\n                   }`}\n                   key={bid.id}\n                 >\n                   {state.isSeller ? (\n                     <Bid_seller\n                       setBidInput={setIsBidPopOpen}\n                       sellerId={bid.sellerId}\n                       logo={bid.logo}\n                       name={bid.sellerName}\n                       desc={bid.description}\n                       amount={bid.amount}\n                     />\n                   ) : (\n                     <Bid_customer\n                       active={productInfo.active}\n                       acceptBidOpen={acceptBidOpen}\n                       setAcceptBidOpen={setAcceptBidOpen}\n                       sellerId={bid.sellerId}\n                       logo={bid.logo}\n                       name={bid.sellerName}\n                       desc={bid.description}\n                       amount={bid.amount}\n                     />\n                   )}\n                 </div>\n               ))}\n             </div>\n           </section>\n\n           {/* Section 3 - Suggested bids */}\n           {state.isSeller && (\n             <section className=\"product__sections\">\n               <h2>Suggested Products to bid</h2>\n               <hr />\n               <div className=\"listings\" style={{ margin: \"0\" }}>\n                 {productInfo.suggestions.map((post) => {\n                   return (\n                     <div\n                       key={post.id}\n                       className=\"product\"\n                       style={{ margin: \"1em 4em 2em\" }}\n                     >\n                       <Link to={`/product/${post.id}`}>\n                         <Post\n                           img={post.img}\n                           name={post.name}\n                           bid={post.lowestBid}\n                           location={post.location}\n                         />\n                       </Link>\n                     </div>\n                   );\n                 })}\n               </div>\n               <hr style={{ marginBottom: \"4rem\" }} />\n             </section>\n           )}\n         </>\n       ) : (\n         <></>\n       )}\n     </div>\n   );\n}\n\nexport default Product;\n",["202","203"],"/home/adithya/Desktop/web-dev/flipin/src/components/private-pages/Orders/BidDetails.js",[],["204","205"],"/home/adithya/Desktop/web-dev/flipin/src/helperFunctions/misc.js",[],"/home/adithya/Desktop/web-dev/flipin/src/components/Routing.js",["206"],"/home/adithya/Desktop/web-dev/flipin/src/components/public-pages/About.js",[],"/home/adithya/Desktop/web-dev/flipin/src/components/staticInfo.js",[],"/home/adithya/Desktop/web-dev/flipin/src/components/public-pages/Post.js",[],["207","208"],"/home/adithya/Desktop/web-dev/flipin/src/components/private-pages/Product/Bid_seller.js",[],"/home/adithya/Desktop/web-dev/flipin/src/components/public-pages/Home.js",["209","210"],"/home/adithya/Desktop/web-dev/flipin/src/components/private-pages/Profile/Profile.js",["211","212"],"/home/adithya/Desktop/web-dev/flipin/src/components/private-pages/Product/Bid_customer.js",[],"/home/adithya/Desktop/web-dev/flipin/src/reducers/userReducer.js",[],["213","214"],"/home/adithya/Desktop/web-dev/flipin/src/components/public-pages/Contact.js",[],["215","216"],"/home/adithya/Desktop/web-dev/flipin/src/components/public-pages/Difference.js",[],"/home/adithya/Desktop/web-dev/flipin/src/components/private-pages/Dashboard/PhotoText.js",[],["217","218"],"/home/adithya/Desktop/web-dev/flipin/src/index.js",[],["219","220"],"/home/adithya/Desktop/web-dev/flipin/