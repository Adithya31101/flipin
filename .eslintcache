ge.ref('images').child(image.name).getDownloadURL()\n               .then(url => {\n                  db.collection(\"chat\")\n                    .add({\n                      message: chatText,\n                      chatroomID: chatDetails.id,\n                      from: currentUser.id,\n                      mediaURL: url,\n                      postedAt: new Date(),\n                    })\n                    .then((res) => {\n                      console.log(\"Message sent successfully!\");\n                    });\n                  setChatText(\"\");\n                  setSrc(\"\");\n                  setImage(null);\n               })\n               .catch(e => console.error(e));\n            }\n         );\n      } else {\n         db.collection(\"chat\")\n           .add({\n             message: chatText,\n             chatroomID: chatDetails.id,\n             from: currentUser.id,\n             mediaURL: null,\n             postedAt: new Date(),\n           })\n           .then((res) => {\n             console.log(\"Message sent successfully!\");\n           });\n         setChatText(\"\");\n      }\n      \n   }\n\n   return (\n      <div className=\"inbox\">\n         <div className=\"inbox__container\">\n            <section className=\"inbox__chatrooms\">\n               <div className=\"chat__search\">\n                  <div className=\"chat__search-box\">\n                     <input type=\"text\" value={searchString} onChange={handleSearchChange} />\n                     <SearchIcon /> \n                  </div>\n               </div>\n               <div className=\"chatrooms\">\n                  {\n                     loading === \"chats\"?\n                     <div className=\"loader\">\n                        <CircularProgress />\n                     </div>\n                     :\n                     chats.map(chat => (\n                        <div onClick={()=>setChatDetails(chat)} className=\"chatroom__chat\" key={chat.id}>\n   