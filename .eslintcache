oute>\n      {state ? (\n        //Private Pages\n        <>\n          {!state.hasAddress ? (\n            <Route>\n              {history.push(\"/profile\")}\n              <Profile />\n            </Route>\n          ) : (\n            <>\n              <Route exact path=\"/dashboard\">\n                <Dashboard />\n              </Route>\n              <Route path=\"/product/:id\">\n                <Product />\n              </Route>\n              <Route path=\"/home\">\n                <Home />\n              </Route>\n              <Route path=\"/orders\">\n                <OrderPage />\n              </Route>\n              <Route path=\"/profile\">\n                <Profile />\n              </Route>\n              <Route path=\"/create\">\n                <CreateListings />\n              </Route>\n              <Route path=\"/browse\">\n                <BrowseListings />\n              </Route>\n            </>\n          )}\n        </>\n      ) : (\n        // Also Public pages that are unaccessible when logged in to avoid jwt override\n        <>\n          <Route exact path=\"/flipin\">\n            <Home />\n          </Route>\n          <Route path=\"/login\">\n            <Login />\n          </Route>\n          <Route path=\"/signup\">\n            <SignUp />\n          </Route>\n        </>\n      )}\n\n      {/* Default Page || 404 page */}\n      <Route>\n        <NotFound />\n      </Route>\n    </Switch>\n  );\n};\n\nexport default Routing;\n",["126","127"],"/home/adithya/Desktop/web-dev/flipin/src/components/public-pages/About.js",[],"/home/adithya/Desktop/web-dev/flipin/src/components/staticInfo.js",[],"/home/adithya/Desktop/web-dev/flipin/src/components/public-pages/Post.js",[],"/home/adithya/Desktop/web-dev/flipin/src/components/private-pages/Product/Bid_seller.js",[],["128","129"],"/home/adithya/Desktop/web-dev/flipin/src/components/public-pages/Home.js",["130","131"],"/home/adithya/Desktop/web-dev/flipin/src/components/private-pages/P